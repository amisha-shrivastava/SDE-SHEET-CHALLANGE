class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        final=[]
        for i in range(1,numRows+1):
                l=[-1]*i
                l[0] = 1
                if i>1:
                  l[i-1] = 1
                final.append(l)
        for l in range(2,numRows):
             for v in range(len(final[l])):
                    if final[l][v] == -1:
                        final[l][v] = final[l-1][v-1]+final[l-1][v]
            
        return final
